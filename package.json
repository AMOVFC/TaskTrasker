{
  "name": "tasktasker",
  "version": "0.0.1",
  "private": true,
  "packageManager": "npm@10.9.0",
  "description": "TaskTasker - Tree-based task management web app",
  "scripts": {
    "dev": "npm --prefix apps/web run dev",
    "build": "npm --prefix apps/web ci && npm --prefix apps/web run build",
    "start": "npm --prefix apps/web run start",
    "deploy": "wrangler pages deploy apps/web/out --project-name tasktasker"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "wrangler": "^4.65.0"
  }
}
